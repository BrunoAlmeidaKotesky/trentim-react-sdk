{
  "name": "trentim-react-sdk",
  "private": false,
  "version": "3.4.0-beta4",
  "description": "Trentim internal SDK for ReactJS projects, with UI components, helpers, hooks and more.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/BrunoAlmeidaKotesky/trentim-react-sdk"
  },
  "scripts": {
    "dev": "vite",
    "build:helpers": "export LIB_NAME=helpers && vite build",
    "build:hooks": "export LIB_NAME=hooks && vite build",
    "build": "yarn build:helpers && yarn build:hooks",
    "preview": "vite preview",
    "release": "yarn build && git add -u && git add -A && git commit --allow-empty -m \"Build changes\" && yarn version && yarn run publish-push",
    "publish-push": "yarn publish --non-interactive && git push && git push --tags",
    "upgrade-release": "yarn upgrade && yarn run release"
  },
  "files": [
    "dist/*",
    "index.d.ts",
    "hooks.d.ts",
    "helpers.d.ts"
  ],
  "module": "./dist/index.esm.js",
  "types": "./index.d.ts",
  "exports": {
    "./helpers": {
      "import": "./dist/helpers/index.js",
      "require": "./dist/helpers/index.cjs"
    },
    "./hooks": {
      "import": "./dist/hooks/index.js",
      "require": "./dist/hooks/index.cjs"
    }
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/node": "^17.0.12",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/styled-components": "^5.1.21",
    "@vitejs/plugin-react": "^2.0.0",
    "acorn-jsx": "^5.3.2",
    "babel-loader": "^8.2.5",
    "json": "^11.0.0",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "rollup-plugin-visualizer": "^5.8.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vite-plugin-build": "^0.6.0",
    "vite-plugin-chunk-split": "^0.2.7",
    "vite-plugin-dts": "^0.9.9"
  },
  "dependencies": {
    "@fluentui/react": "8.52.3",
    "bakutils-types": "^1.1.0",
    "fast-sort": "^3.1.3",
    "react-is": "16.8.0",
    "styled-components": "^5.3.5"
  }
}
