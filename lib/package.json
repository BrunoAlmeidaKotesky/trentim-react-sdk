{
  "name": "trentim-react-sdk",
  "private": false,
  "version": "4.2.0",
  "description": "Trentim internal SDK for ReactJS projects, with UI components, helpers, hooks and more.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/BrunoAlmeidaKotesky/trentim-react-sdk"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "release": "yarn build && git add -u && git add -A && git commit --allow-empty -m \"Build changes\" && yarn version && yarn run publish-push",
    "publish-push": "yarn publish --non-interactive && git push && git push --tags",
    "upgrade-release": "yarn upgrade && yarn run release",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    "./ConditionalWrapper": {
      "import": "./dist/ConditionalWrapper.es.js",
      "require": "./dist/ConditionalWrapper.cjs.js"
    },
    "./GridView": {
      "import": "./dist/GridView.es.js",
      "require": "./dist/GridView.cjs.js"
    },
    "./IFrame": {
      "import": "./dist/IFrame.es.js",
      "require": "./dist/IFrame.cjs.js"
    },
    "./LifecycleProgress": {
      "import": "./dist/LifecycleProgress.es.js",
      "require": "./dist/LifecycleProgress.cjs.js"
    },
    "./StickerCard": {
      "import": "./dist/StickerCard.es.js",
      "require": "./dist/StickerCard.cjs.js"
    },
    "./Tooltip": {
      "import": "./dist/Tooltip.es.js",
      "require": "./dist/Tooltip.cjs.js"
    },
    "./decorators": {
      "import": "./dist/decorators.es.js",
      "require": "./dist/decorators.cjs.js"
    },
    "./helpers": {
      "import": "./dist/helpers.es.js",
      "require": "./dist/helpers.cjs.js"
    },
    "./hooks": {
      "import": "./dist/hooks.es.js",
      "require": "./dist/hooks.cjs.js"
    },
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./models": {
      "import": "./dist/models.es.js",
      "require": "./dist/models.cjs.js"
    }
  },
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "ConditionalWrapper.d.ts",
    "GridView.d.ts",
    "IFrame.d.ts",
    "LifecycleProgress.d.ts",
    "StickerCard.d.ts",
    "Tooltip.d.ts",
    "decorators.d.ts",
    "helpers.d.ts",
    "hooks.d.ts",
    "index.d.ts",
    "models.d.ts"
  ],
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/node": "^17.0.12",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@vitejs/plugin-react": "^2.0.0",
    "acorn-jsx": "^5.3.2",
    "babel-loader": "^8.2.5",
    "json": "^11.0.0",
    "react": "^16.9.0",
    "react-dom": "16.9.0",
    "rollup-plugin-visualizer": "^5.8.0",
    "sass": "^1.56.0",
    "typescript": "^4.6.4",
    "vite": "3.2.3",
    "vite-plugin-build": "^0.6.0",
    "vite-plugin-chunk-split": "^0.2.7",
    "vite-plugin-css-injected-by-js": "^2.1.1",
    "vite-plugin-dts": "1.7.0",
    "vite-plugin-libcss": "^1.0.5",
    "vitest": "^0.23.4"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "dependencies": {
    "@fluentui/react": "^8.52.3",
    "@fluentui/react-icons": "^2.0.186",
    "bakutils-types": "^1.1.0",
    "fast-sort": "^3.1.3"
  }
}
